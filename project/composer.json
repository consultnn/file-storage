{
    "name": "consultnn/file-storage",
    "description": "Simple file storage",
    "keywords": ["slim", "file storage", "image service"],
    "homepage": "http://github.com/consultnn/file-storage",
    "license": "MIT",
    "authors": [],
    "require": {
        "php": "~7.0.9",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
